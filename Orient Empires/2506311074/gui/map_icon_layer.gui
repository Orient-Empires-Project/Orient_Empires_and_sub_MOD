	text_label_center = {
		parentanchor = hcenter
		visible = "[Not( GuiHolySiteItem.IsHeldByFaith )]"
		text = "[HolySite.GetBarony.GetNameNoTierNoTooltip]"
		default_format = "#medium"
	}
}

